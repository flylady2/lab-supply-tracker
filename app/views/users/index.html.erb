<h2><%=@lab.principal_investigator%> Lab members:</h2>
  <% @users.each do |user| %>
    <ul>
      Name:<%= user.name %><br>
      Email:<%= user.email %><br>
      <%= link_to 'Delete',
        user_path(user),
          method: :delete,
          data: { confirm: 'Are you sure?'} %>
    </ul>
  <% end %>
